$(document).ready(function() {
	"use strict";

	var ENDPOINT = "http://localhost:3000/tsasks";

	$(document).ajaxError(function(){
		console.log("error:", arguments);
		alert("Error!");
	});

	$.ajax(ENDPOINT, {
		method: "GET",
		data: {
			title: "task1"
		},
		dataType: "json"
	}).then(function(responce){
		console.log(responce);
	});
})