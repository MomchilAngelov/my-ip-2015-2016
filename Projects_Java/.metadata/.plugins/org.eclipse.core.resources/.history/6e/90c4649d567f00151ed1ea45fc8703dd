package tinker;

import java.util.Scanner;


public class Main {
	
	public static void main(String[] args){
		System.out.println("Starting!");
//		DataHolder holder = new DataHolder();
		Command commander = new Command();
		commander.commander.put("login", new Login());
		commander.commander.put("logout", new Logout());
		commander.commander.put("info", new Info());
		
		
		final Scanner in = new Scanner(System.in);
		final String name = in.next();
		int i = 0;
		while(true){
			final String[] command = name.split(":");
			if(commander.commander.containsKey(command[1])){
				commander.commander.get(command[1]).execute(command);
				continue
			}
			i++;
			if(i == 5){
				break;
			}
		}
		in.close();
		
	}
}
