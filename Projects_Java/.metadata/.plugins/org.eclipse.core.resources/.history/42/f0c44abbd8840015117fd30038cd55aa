package socket;

import java.io.IOException;
import java.net.Socket;

public class Server {
	private static final int SERVER_PORT = 31111;
	
	public static void main(String[] args) throws IOException {
		//final EchoServer server = new EchoServer(SERVER_PORT);
		final Socket socket = new Socket("localhost", SERVER_PORT);
		final StreamWriter out =
		        new StreamWriter(socket.getOutputStream(), true);
	}
	
}
