		total_info.put("Momo", some_setup[4]);
		for (Map.Entry<String, Student> entry : total_info.entrySet()) {
			//entry.getValue().set_name(entry.getKey());
			System.out.println(entry);
		}
		
		some_setup[0].set_age(22);
		some_setup[0].info();
		
