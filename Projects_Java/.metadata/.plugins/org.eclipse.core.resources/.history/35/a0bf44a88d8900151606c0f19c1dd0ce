package tinker;

import java.util.Map;
import java.util.Date;

public class User {
	private String name;
	private Integer timesInClass = 0;
	private Map<String, String> dates = new HashMap<String, String>();
	
	User(String name){
		this.name = name;
	}
	
	public void increaseTimesInClass(){
		timesInClass++;
	}
	
	public String getName(){
		return this.name;
	}
	
	
}
