package onepager;

import java.util.Calendar;
import java.util.HashMap;
import java.util.Map;
import java.util.Scanner;
import java.time.LocalDateTime;

public class OneInAll {
	public class Student{
		String name;
		int age;
		boolean is_in = false;
		LocalDateTime time_in;
		LocalDateTime time_out;
		
		Map<String, String> info(){
			Map<String, String> myMap = new HashMap<String, String>();
			myMap.put("In: ", time_in.toString());
			return myMap;
		}
		
		void in(){
			is_in = true;
			time_in = LocalDateTime.now();
		}
		
		void out(){
			is_in = false;
			time_out = LocalDateTime.now();
		}
		
	}
	public static void main(String[] args){
		System.out.println("My stuff");
		final Map<String, Integer> nameToCountMap =
				new HashMap<String, Integer>();
		
		//nameToCountMap.put("Ivan", 1);
		
		//System.out.println(nameToCountMap.get("Ivan"));
		System.out.println("Enter a bunch of names: ");
		int i = 0;
		final Scanner in = new Scanner(System.in);
		while(true){
			
			final String key = in.next();
			if ((i == 30) || (key.equals("close"))){
				break;
			}
			//System.out.println("Input: " + key);
			if(nameToCountMap.containsKey(key)){
			nameToCountMap.put(key, nameToCountMap.get(key) + 1);
			} else {
				nameToCountMap.put(key, 1);
			}
		}
		
		in.close();
		for (Map.Entry<String, Integer> entry : nameToCountMap.entrySet()) {
		    System.out.println(entry);
		}
		
		LocalDateTime some_value = LocalDateTime.now();
		System.out.println(some_value);
		some_value = some_value.toString();
		System.out.println(some_value);
		//System.out.printf("Hello, %s!",name );
	}
}
