package onepager;

import java.util.HashMap;
import java.util.Map;
import java.util.Scanner;

public class OneInAll {
	public static void main(String[] args){
		System.out.println("My stuff");
		final Map<String, Integer> nameToCountMap =
				new HashMap<String, Integer>();
		
		//nameToCountMap.put("Ivan", 1);
		
		//System.out.println(nameToCountMap.get("Ivan"));
		System.out.println("Enter a bunch of names: ");
		int i = 0;
		final Scanner in = new Scanner(System.in);
		while(true){
			final String key = in.next();
			System.out.println("Input: " + key);
			nameToCountMap.put(key, nameToCountMap.get(key) + 1);
			if ((i == 30) || (name.equals("close"))){
				break;
			}
		}
		
		in.close();
		
		for (nameToCountMap: nameToCountMap.entrySet())
		{
		    System.out.println(entry.getKey() + "=>" + entry.getValue());
		}
		
		//System.out.printf("Hello, %s!",name );
	}
}
